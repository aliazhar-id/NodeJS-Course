    <h1>Web Server NodeJS</h1>
    <h2>Halo <%= nama %></h2>
    
    <h4>Daftar Mahasiswa</h4>
    <% if (mahasiswa.length < 1) { %>
    <p>Data mahasiswa masih kosong</p>
    <% } else { %>
    <% mahasiswa.forEach( ({nama, email}) => {%>
    <ul>
      <li>Nama : <%= nama %></li>
      <li>Email : <%= email %></li>
    </ul>
    <%})%>
    <% } %>
    